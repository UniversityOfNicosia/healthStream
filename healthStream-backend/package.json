{
  "name": "health-stream-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "gesy-parse": "./node_modules/.bin/ts-node-dev --transpile-only  ./src/scripts/importXML.ts",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn  --no-notify ./src/index.ts",
    "start": "node ./build/index.js",
    "compile:ts": "tsc -p  ./tsconfig.build.json",
    "build": "npm run clean:build && npm run compile:ts",
    "clean:build": "rimraf build",
    "test": "NODE_OPTIONS=--max-old-space-size=3192 TS_NODE_TRANSPILE_ONLY=true mocha && exit 1",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migration:gen": "npm run typeorm migration:generate -- -n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "reset:db": "ts-node src/scripts/database.ts --reset",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config -n src/ormconfig.ts",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n src/ormconfig.ts",
    "seed": "npm run seed:config && npm run seed:run",
    "ts-node": "TS_NODE_TRANSPILE_ONLY=true  ts-node"
  },
  "dependencies": {
    "@types/gapi": "0.0.39",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "camelcase-keys": "^6.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "googleapis": "^67.1.1",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "on-finished": "^2.3.0",
    "path": "^0.12.7",
    "query-string": "^6.14.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "tslib": "^2.1.0",
    "typeorm": "0.2.29",
    "typeorm-naming-strategies": "2.0.0",
    "typeorm-seeding": "^1.2.0",
    "url-parse": "^1.5.1",
    "util": "^0.12.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/he": "^1.1.1",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/on-finished": "^2.3.1",
    "@types/request": "^2.48.5",
    "@types/supertest": "^2.0.10",
    "@types/url-parse": "^1.4.3",
    "@types/xml2js": "^0.4.8",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "ts-node-dev": "1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
